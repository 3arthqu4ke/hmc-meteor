{
  "schemaVersion": 1,
  "id": "hmc-meteor",
  "version": "${version}",
  "name": "HMC-Meteor",
  "description": "An Addon for Meteor that adds HeadlessMc support.",
  "authors": [
    "3arthqu4ke"
  ],
  "contact": {
    "repo": "https://github.com/3arthqu4ke/hmc-meteor"
  },
  "icon": "assets/hmcmeteor/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "me.earth.hmcmeteor.HmcMeteorAddon"
    ]
  },
  "mixins": [
    "hmcmeteor.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}"],
    "meteor-client": "*",
    "headlessmc": "*"
  }
}
